		[
			rr:sqlQuery """
				SELECT * 
				FROM uasgs_do_ceara
				WHERE ativo is false
			"""
		];
	rr:subjectMap
		[
			rr:class ardo:UASGInativa;
			rr:template "http://www.arida.com/siasg/resources/UASG/{id}"
		].

