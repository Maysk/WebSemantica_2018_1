			rr:objectMap [rr:column '"fundamento_legal"']
		];

	rr:predicateObjectMap 

		[
			rr:predicate ardo:numero_processo;
			rr:objectMap [rr:column '"numero_processo"']
		];

	rr:predicateObjectMap 

		[
			rr:predicate ardo:valor_inicial;
			rr:objectMap [rr:column '"valor_inicial"']
		];

	rr:predicateObjectMap 

